{
    "name": "officelifehq/officelife",
    "type": "project",
    "description": "Know how your employees feel.",
    "keywords": [
        "company",
        "crm",
        "employee lifecycle"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.1",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "mariuzzo/laravel-js-localization": "^1.7",
        "moneyphp/money": "^4.5",
        "monicahq/laravel-cloudflare": "^3.2",
        "parsedown/laravel": "^1.2",
        "sentry/sentry-laravel": "^4.6",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/laravelpassport": "^4.1",
        "socialiteproviders/linkedin": "^4.1",
        "socialiteproviders/microsoft-azure": "^5.2",
        "socialiteproviders/slack": "^5.0.0",
        "socialiteproviders/twitter": "^4.1",
        "spatie/laravel-ignition": "^2.0",
        "spatie/simple-excel": "^3.6",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1",
        "tightenco/ziggy": "^2.2",
        "uploadcare/uploadcare-php": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-dump-server": "^2.0",
        "fakerphp/faker": "^1.10",
        "graham-campbell/testbench-core": "^4.1",
        "laravel/dusk": "^8.2",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/AccountHelper.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
